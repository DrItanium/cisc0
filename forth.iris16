;-----------------------------------------------------------------------------
; Fixed registers (this is in the hardware)
;   - InstructionPoiner : r255
;	- LinkRegister: r254
; 	- StackPointer: r253
; register conventions
;	- r252 - Return Stack
;	- r251 - Parameter Stack
;-----------------------------------------------------------------------------



;-----------------------------------------------------------------------------
; General return routine
;-----------------------------------------------------------------------------
@label return
	pop r130
	goto r130
;-----------------------------------------------------------------------------
; Indirect call routine
; Inputs 
;	- r128: destination to jump to
;	- r129: where we came from
;-----------------------------------------------------------------------------
@label call
	push r129
	goto r128

;-----------------------------------------------------------------------------
; Push a value onto the return stack
;-----------------------------------------------------------------------------
@label push_return_stack
	
